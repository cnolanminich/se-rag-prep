# GitHub API Token
# Generate at: https://github.com/settings/tokens
# Required scopes: repo (for private repos) or public_repo (for public repos only)
GITHUB_TOKEN=your_github_token_here

# Notion Integration Token
# Create at: https://www.notion.so/my-integrations
# Must have access to the pages/databases you want to process
NOTION_TOKEN=your_notion_integration_token_here

# ScoutOS API Key
# Get from your ScoutOS account
SCOUTOS_API_KEY=your_scoutos_api_key_here

# ScoutOS Collection and Table IDs for GitHub content
# These will be populated after running the GitHub component setup assets
SCOUTOS_COLLECTION_ID=your_github_collection_id_here
SCOUTOS_TABLE_ID=your_github_table_id_here

# ScoutOS Collection and Table IDs for Notion content  
# These will be populated after running the Notion component setup assets
SCOUTOS_NOTION_COLLECTION_ID=your_notion_collection_id_here
SCOUTOS_NOTION_TABLE_ID=your_notion_table_id_here